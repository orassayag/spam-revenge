    "type": "git",
    "url": "git://github.com/orassayag/spam-revenge.git"
  },
  "keywords": [
    "spam",
    "newsletter",
    "newsletters",
    "email",
    "email address",
    "email addresses",
    "subscribe",
    "proxy"
  ],
  "license": "UNLICENSED",
  "author": "Or Assayag <orassayag@gmail.com>",
  "contributors": [
    {
      "name": "Or Assayag",
      "email": "orassayag@gmail.com",
      "url": "https://github.com/orassayag"
    }
  ],
  "main": "src/scripts/subscribe.script.js",
  "files": [
    "src",
    ".eslintignore",
    ".eslintrc",
    ".gitignore",
    ".jsbeautifyrc",
    ".npmignore",
    "INSTRUCTIONS.md",
    "package-lock.json",
    "package.json",
    "README.md"
  ],
  "scripts": {
